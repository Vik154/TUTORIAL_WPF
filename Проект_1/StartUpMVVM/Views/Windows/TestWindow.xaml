<Window x:Class="StartUpMVVM.Views.Windows.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartUpMVVM.Views.Windows"
        mc:Ignorable="d"
        Title="TestWindow" 
        Height="450" Width="800"
        WindowStyle="None"
        Style="{StaticResource TestWindowStyle}"
        >
    <Grid>
        <Button VerticalAlignment="Top"
                HorizontalAlignment="Left"
                Margin="30,40"
                Padding="20,5"
                Content="Hello world">
        </Button>
        <Canvas>
            <Ellipse Canvas.Left="20" Canvas.Bottom="20"
                     StrokeThickness="1" Stroke="Blue"
                     Fill="Red" Width="30" Height="20">
            </Ellipse>
        </Canvas>
    </Grid>
</Window>
