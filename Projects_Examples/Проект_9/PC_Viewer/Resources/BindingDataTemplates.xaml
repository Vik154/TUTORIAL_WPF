<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:PC_Viewer.ViewModels"
                    xmlns:v="clr-namespace:PC_Viewer.Views">

    <!--Desktop Page-->
    <DataTemplate DataType="{x:Type vm:DesktopViewModel}">
        <v:DesktopView/>
    </DataTemplate>

    <!--Home Page-->
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <v:HomeView/>
    </DataTemplate>

    <!--Document Page-->
    <DataTemplate DataType="{x:Type vm:DocumentViewModel}">
        <v:DocumentView/>
    </DataTemplate>

    <!--Download Page-->
    <DataTemplate DataType="{x:Type vm:DownloadViewModel}">
        <v:DownloadView/>
    </DataTemplate>

    <!--Movie Page-->
    <DataTemplate DataType="{x:Type vm:MovieViewModel}">
        <v:MovieView/>
    </DataTemplate>

</ResourceDictionary>